{
	"schemaVersion": 1,
	"id": "keybindsgaloreplus",
	"version": "${version}",

	"name": "KeybindsGalore Plus",
	"description": "Adds a popup pie menu when a key with conflicting bindings is pressed. A fork of KeybindsGalore, originally by Cael and updated to 1.20 by HVB007",
	"authors": [
		"Blender",
		"HVB007",
		"Cael"
	],
	"contributors": [
		"PoopooRacoocoo -- Java incompatibility issue",
		"Mo9713 & customjack -- editable keybind description",
		"GabanKillasta -- compatibility with zoom mods",
		"GhostIsBeHere -- key blacklist",
		"Alwis2000 -- flagging yet another Java incompatibility mistake",
		"GuardedHoney53 -- the very funny setPressed issue",
		"ClutchMasterYT",
		"BumbleTree -- Bad SemVer",
		"Mideks -- Console spam",
		"UNI717 -- Sinytra Connector compatibility",
		"IG114514 -- Mouse support and compatibility",
		"...and more on GitHub!"
	],
	"license": "GNU Lesser General Public License v3.0",
	"icon": "assets/keybindsgaloreplus/icon.png",

	"environment": "client",
	"entrypoints": {
		"client": [ "me.av306.keybindsgaloreplus.KeybindsGalorePlus" ]
	},
	"mixins": [
      "keybindsgaloreplus.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.8",
		"minecraft": ">=1.21 <=1.21.1",
		"minecraft-": ">=1.21.2",
		"minecraft--": ">=1.20 <=1.20.1",
		"minecraft---": ">=1.20.2 <=1.20.6"
	}
}
